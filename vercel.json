{
  "version": 2,
  "buildCommand": "echo 'skip build'",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "client/public/images/$1"
    },
    {
      "src": "/(?!api)(.+\\.(js|jsx|css|json|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))$",
      "dest": "client/$1"
    },
    {
      "src": "/style\\.css$",
      "dest": "client/style.css"
    },
    {
      "src": "/app\\.jsx$",
      "dest": "client/app.jsx"
    },
    {
      "src": "/index\\.html$",
      "dest": "client/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "client/index.html"
    }
  ]
}