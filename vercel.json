{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "api/index.js"
    },
    {
      "src": "/images/.*",
      "dest": "api/index.js"
    },
    {
      "src": "/.*\\.(jsx?|css|json|png|jpg|jpeg|gif|svg|ico)$",
      "dest": "api/index.js"
    },
    {
      "src": "/.*",
      "dest": "api/index.js"
    }
  ]
}