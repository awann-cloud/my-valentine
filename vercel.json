{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/images/(.*)",
      "destination": "/client/public/images/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/client/$1"
    },
    {
      "source": "/",
      "destination": "/client/index.html"
    }
  ]
}