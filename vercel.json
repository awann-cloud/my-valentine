{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "client/public/images/$1"
    },
    {
      "src": "/(.*\\.jsx)$",
      "dest": "client/$1"
    },
    {
      "src": "/(.*\\.css)$",
      "dest": "client/$1"
    },
    {
      "src": "/(.*\\.js)$",
      "dest": "client/$1"
    },
    {
      "src": "/(.*\\.json)$",
      "dest": "client/$1"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|woff|woff2))$",
      "dest": "client/$1"
    },
    {
      "src": "/index.html",
      "dest": "client/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "client/index.html"
    }
  ]
}