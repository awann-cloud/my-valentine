{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "client/public/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}